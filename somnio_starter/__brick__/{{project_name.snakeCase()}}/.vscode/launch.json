  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch development",
      "request": "launch",
      "type": "dart",
      "program": "lib/main/main_development.dart",
      "args": [
        "--flavor",
        "development",
        "--target",
        "lib/main/main_development.dart",
        "--dart-define-from-file",
        "app-keys.dev.json"
      ]
    },
    {
      "name": "Launch staging",
      "request": "launch",
      "type": "dart",
      "program": "lib/main/main_staging.dart",
      "args": [
        "--flavor",
        "staging",
        "--target",
        "lib/main/main_staging.dart",
        "--dart-define-from-file",
        "app-keys.stg.json"
      ]
    },
    {
      "name": "Launch production",
      "request": "launch",
      "type": "dart",
      "program": "lib/main/main_production.dart",
      "args": [
        "--flavor",
        "production",
        "--target",
        "lib/main/main_production.dart",
        "--dart-define-from-file",
        "app-keys.prod.json"
      ]
    },
    {
      "name": "Launch web development",
      "request": "launch",
      "type": "dart",
      "program": "lib/main/main_development.dart",
      "args": [
        "--target",
        "lib/main/main_development.dart",
        "--dart-define-from-file",
        "app-keys.dev.json"
      ]
    },
    {
      "name": "Launch web staging",
      "request": "launch",
      "type": "dart",
      "program": "lib/main/main_staging.dart",
      "args": [
        "--target",
        "lib/main_staging.dart",
        "--dart-define-from-file",
        "app-keys.stg.json"
      ]
    },
    {
      "name": "Launch web production",
      "request": "launch",
      "type": "dart",
      "program": "lib/main/main_production.dart",
      "args": [
        "--target",
        "lib/main_production.dart",
        "--dart-define-from-file",
        "app-keys.prod.json"
      ]
    },
    {
      "name": "Launch gallery",
      "request": "launch",
      "type": "dart",
      "program": "packages/{{project_name.snakeCase()}}_ui/gallery/lib/main.dart"
    }
  ]
}



